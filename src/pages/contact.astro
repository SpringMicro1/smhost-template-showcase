---
import Layout from "@/layouts/Layout.astro"
import json from "@/content/forms/contact.json";
import JSONForm from "@/components/forms";


---

<Layout title="Contact" description="Contact us."><div class="grid grid-cols-[1fr] grid-rows-[1fr] gap-x-0 gap-y-0">
    <div class="text-center">
  <h1>Contact Us</h1>
</div><div class="px-3 mx-auto w-full max-w-lg mb-2">
  <JSONForm json={json} env={Object.fromEntries(Object.entries(import.meta.env).filter(([k,v])=> k.indexOf("PUBLIC_") === 0))} client:load />
</div>
  </div></Layout>

  